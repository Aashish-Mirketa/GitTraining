<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_ZipCode</fullName>
    <active>true</active>
    <errorConditionFormula>OR(NOT(ISNUMBER( ZipCode__c )), LEN( ZipCode__c )&lt;&gt;5)</errorConditionFormula>
    <errorMessage>Zip Code must be in 5-digit format (0-
9)</errorMessage>
</ValidationRule>
